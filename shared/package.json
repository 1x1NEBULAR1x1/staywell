{
  "name": "shared",
  "version": "1.0.0",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "types": "src/index.d.ts",
      "default": "src/index.js"
    }
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "argon2": "^0.43.1",
    "lucide-react": "^0.544.0",
    "prisma": "^6.13.0"
  },
  "scripts": {
    "build": "tsc --build --force",
    "clean": "find src -path \"src/database\" -prune -o -name \"*.js\" -print -o -name \"*.d.ts\" -print | xargs rm -f",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "npx prisma db push --force-reset"
  },
  "devDependencies": {
    "typescript": "^5.8.3"
  },
  "prisma": {
    "schema": "./prisma"
  }
}